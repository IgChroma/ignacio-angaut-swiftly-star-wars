{
  "compilerOptions": {
    "strict": true,
    "target": "ES6",
    "module": "ESNext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "jsx": "react",
    "paths": {
      "jquery": [
        "./vendor/jquery/dist/jquery"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@style/*": [
        "./src/style/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ]
    },
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  },
}